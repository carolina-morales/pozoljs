{
	"name": "@daniel-cmorales/pozoljs",
	"version": "0.4.1",
	"description": "Initialize a node-express project with typescript or javascript and create full components to develop your backend apps faster.",
	"main": "src/index.js",
	"bin": {
		"@daniel-cmorales/pozoljs": "./bin/pozoljs",
		"pozoljs": "./bin/pozoljs"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "tsc -p . && npm run copy-integrations && npm run copy-templates",
		"build:ts": "tsc -p .",
		"build:copy": "npm run copy-integrations && npm run copy-templates",
		"start": "npm run build && npm link",
		"copy-integrations": "copyfiles -u 1 -a \"src/integrations/**/*.*\" build",
		"copy-templates": "copyfiles -u 1 \"src/schematics/templates/*\" build",
		"publish-package": "npm run build && npm publish"
	},
	"keywords": [
		"cli",
		"create-pozol-project",
		"pozoljs",
		"pozol js",
		"pozol",
		"pozol-app"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/daniel-cmorales/pozol-js.git"
	},
	"author": "Daniel Morales, <daniel.cmorales3@gmai.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/daniel-cmorales/pozol-js/issues"
	},
	"homepage": "https://github.com/daniel-cmorales/pozol-js#readme",
	"dependencies": {
		"chalk": "^4.1.0",
		"cli-table3": "^0.6.0",
		"commander": "^6.1.0",
		"copyfiles": "^2.3.0",
		"ejs": "^3.1.5",
		"execa": "^4.0.3",
		"figlet": "^1.5.0",
		"inquirer": "^7.3.3",
		"listr": "^0.14.3",
		"ncp": "^2.0.0"
	},
	"devDependencies": {
		"@types/ejs": "^3.0.4",
		"@types/execa": "^2.0.0",
		"@types/figlet": "^1.2.0",
		"@types/inquirer": "^7.3.1",
		"@types/listr": "^0.14.2",
		"@types/ncp": "^2.0.4",
		"@types/node": "^14.10.1",
		"typescript": "^4.0.2"
	}
}
